{
    "name": "@plurid/enhanced-grpc",
    "version": "0.0.3",
    "description": "gRPC Utilities and Enhancements",
    "keywords": [
        "enhanced",
        "grpc"
    ],
    "author": "Jan Cavel <caveljan@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-grpc"
    },
    "bugs": {
        "email": "feedback@plurid.com",
        "url": "https://github.com/plurid/enhanced-grpc/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "jsnext:main": "distribution/index.es.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "spec": "jest ./source",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c -w --environment MODE_ENV:local",
        "build.local": "yarn clean && rollup -c --environment MODE_ENV:local",
        "build.development": "yarn clean && rollup -c --environment MODE_ENV:development",
        "build.production": "yarn clean && rollup -c --environment MODE_ENV:production",
        "build": "yarn lint && yarn spec && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@grpc/proto-loader": ">=0.5",
        "grpc": ">=1.24"
    },
    "devDependencies": {
        "@grpc/proto-loader": "^0.5.4",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.2",
        "@types/jest": "^25.2.1",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "grpc": "^1.24.2",
        "jest": "^25.5.4",
        "rollup": "^2.7.6",
        "rollup-plugin-peer-deps-external": "^2.2.2",
        "rollup-plugin-typescript2": "^0.27.0",
        "ts-jest": "^25.4.0",
        "typescript": "^3.8.3"
    }
}
