{
    "name": "@plurid/enhanced-grpc",
    "version": "0.0.0-0",
    "description": "gRPC Utilities and Enhancements",
    "keywords": [
        "enhanced",
        "grpc"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/enhanced-grpc"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/enhanced-grpc/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "clean": "rm -rf ./distribution",
        "spec": "jest ./source",
        "lint": "eslint ./source --ext .ts,.tsx",
        "start": "yarn clean && rollup -c -w --environment ENV_MODE:local",
        "build.local": "yarn clean && rollup -c --environment ENV_MODE:local",
        "build.development": "yarn clean && rollup -c --environment ENV_MODE:development",
        "build.production": "yarn clean && rollup -c --environment ENV_MODE:production",
        "build": "yarn lint && yarn spec && yarn build.production",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@grpc/proto-loader": ">=0.5",
        "grpc": ">=1.24"
    },
    "devDependencies": {
        "@grpc/proto-loader": "^0.5.5",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-replace": "^2.3.4",
        "@types/jest": "^26.0.15",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.14.0",
        "grpc": "^1.24.4",
        "jest": "^26.6.3",
        "rollup": "^2.34.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.2"
    }
}
